{
  "name": "gulp-fileindex",
  "version": "0.0.0",
  "description": "Outputs a list of pages links in an HTML page",
  "scripts": {
    "dev": "npm run watch",
    "lint": "eslint index.js tests/*.js",
    "test": "mocha tests/tests.js",
    "watch": "watchify **/*.js -o 'npm run lint && npm run test'"
  },
  "engines": {
    "node": ">=0.10.x"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raiseandfall/gulp-fileindex.git"
  },
  "keywords": [
    "gulpplugin",
    "file",
    "files",
    "index",
    "html",
    "links",
    "styleguide",
    "static",
    "gulp"
  ],
  "author": {
    "name": "raiseandfall",
    "url": "https://github.com/raiseandfall"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raiseandfall/gulp-fileindex/issues"
  },
  "homepage": "https://github.com/raiseandfall/gulp-fileindex",
  "devDependencies": {
    "eslint": "^2.5.3",
    "gulp": "^3.9.1",
    "mocha": "^2.4.5",
    "watchify": "^3.7.0"
  }
}
